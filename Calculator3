public class Calculator3 {
    public static void main(String[] args) {

        int number1;
        int number2;
        int number3, number4;
        int result2;

        number1 = 4;
        number2 = 8;
        number3 = 5;
        number4 = 6;

        int result = number1+number2;
        result2 = number3+number4;

        System.out.println(result + result2);
    }
}
